n=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters> \n  <!--Overlay-->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <!--Container-->\n<h1 mbr-if=\"showTitle\">3D IMAGE View</h1><br>\n<div class=\"container\">\n    <div id=\"carousel\">\n        <figure mbr-if=\"dimgslidercount > 0\"><a href=\"index.html#PST-Ent_Techno\"><img src=\"@PROJECT_PATH@/assets/images/pst-slide-tech9.jpg\" alt=\"PST - Kỹ thuật cơ khí công nghiệp và dân dụng\"></a></figure>\n        <figure mbr-if=\"dimgslidercount > 1\"><a href=\"index.html#PST-Ent_Techno\"><img src=\"@PROJECT_PATH@/assets/images/pst-slide-tech8.jpg\" alt=\"PST - Bộ lọc tĩnh điện\"></a></figure>\n        <figure mbr-if=\"dimgslidercount > 2\"><a href=\"index.html#PST-Ent_Techno\"><img src=\"@PROJECT_PATH@/assets/images/pst-slide-tech7.jpg\" alt=\"PST - Bộ lọc bụi làm bằng vật liệu rỗng\"></a></figure>\n        <figure mbr-if=\"dimgslidercount > 3\"><a href=\"index.html#PST-Ent_Techno\"><img src=\"@PROJECT_PATH@/assets/images/pst-slide-tech6.jpg\" alt=\"PST - Bộ lọc bụi kiểu sủi bọt\"></a></figure>\n        <figure mbr-if=\"dimgslidercount > 4\"><a href=\"index.html#PST-Ent_Techno\"><img src=\"@PROJECT_PATH@/assets/images/pst-slide-tech5.jpg\" alt=\"PST - Bộ lọc bụi kiểu thùng quay\"></a></figure>\n        <figure mbr-if=\"dimgslidercount > 5\"><a href=\"index.html#PST-Ent_Techno\"><img src=\"@PROJECT_PATH@/assets/images/pst-slide-tech4.jpg\" alt=\"PST - Bộ lọc bụi kiểu lưới\"></a></figure>\n        <figure mbr-if=\"dimgslidercount > 6\"><a href=\"index.html#PST-Ent_Techno\"><img src=\"@PROJECT_PATH@/assets/images/pst-slide-tech3.jpg\" alt=\"PST - Bộ lọc bụi kiểu túi vải\"></a></figure>\n        <figure mbr-if=\"dimgslidercount > 7\"><a href=\"index.html#PST-Ent_Techno\"><img src=\"@PROJECT_PATH@/assets/images/pst-slide-tech2.jpg\" alt=\"PST - Bộ lọc bụi kiểu quá